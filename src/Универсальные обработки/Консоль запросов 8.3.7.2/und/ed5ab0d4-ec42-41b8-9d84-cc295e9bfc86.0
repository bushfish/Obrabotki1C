Процедура ПроверкаФоновогоЗадания()
	
	Результат = ПроверкаФоновогоЗаданияНаСервере();
	Если Результат = Неопределено Тогда
		СброситьПроцесс(Неопределено);
	Иначе
		Если НЕ Результат.Состояние = ""Задание выполняется"" Тогда
			СброситьПроцесс(Неопределено);
		КонецЕсли;
	КонецЕсли;
	//Состояние(""Выполняется алгоритм..."",ПрогрессОбработки);
	//Состояние(""Выполняется запрос..."" + Результат.Состояние, Результат.Прогресс);
	
	
	Если Результат.Состояние = ""Задание выполнено"" Тогда
		//Данные = ПолучитьИзВременногоХранилища(Результат.АдресРезультата);
		ВывестиРезультат();
		НастроитьТабДокПослеВывода();
		УдалитьИзВременногоХранилища(ПутьКХранилищуИсходный);
		Состояние(""Запрос выполнен."", , , Элементы.ДекорацияИнформация.Картинка); 
	КонецЕсли; 
	
