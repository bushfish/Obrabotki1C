&НаКлиенте
Процедура ВложитьЗапросПакета(Команда)
	
	//ТекущиеДанные = ДеревоЗапросов.НайтиПоИдентификатору(Элементы.ДеревоЗапросов.ТекущаяСтрока);
	ТекущиеДанные = Элементы.ДеревоЗапросов.ТекущиеДанные;
	
	Если ТекущиеДанные = Неопределено Тогда
		Возврат;
	КонецЕсли; 
	
	ИсходныйТекстПакета = Элементы.ДеревоЗапросовТекст.ВыделенныйТекст;
	
	Если ПустаяСтрока(ИсходныйТекстПакета) Тогда
		Возврат;
	КонецЕсли;
	
	НовыйТекстПакета = ВложитьЗапросПакетаНаСервере(ИсходныйТекстПакета);
	
	Если ПустаяСтрока(НовыйТекстПакета) Тогда
		Возврат;
	КонецЕсли;
	
	НачалоСтроки  = 0; НачалоКолонки = 0; КонецСтроки   = 0; КонецКолонки  = 0;
	Элементы.ДеревоЗапросовТекст.ПолучитьГраницыВыделения(НачалоСтроки, НачалоКолонки, КонецСтроки, КонецКолонки);

	ПоследняяяСтрокаВыделенияБезСимволов = КонецКолонки = 1;
	
	ИсходныйТекстЗапроса = ТекущиеДанные.Текст;
	
	ТекстовыйДокументИсходногоЗапроса = Новый ТекстовыйДокумент;
	ТекстовыйДокументИсходногоЗапроса.УстановитьТекст(ИсходныйТекстЗапроса);
	
	ТекстДо = """";
	//КоэфДо = 0;
	Если НачалоСтроки > 1 ИЛИ НачалоКолонки > 1 Тогда
		//КоэфДо = ?(НачалоКолонки = 1, 1, 0);
		//НачалоСтроки = ?(НачалоКолонки - 1 = 0, НачалоСтроки - 1, НачалоСтроки);
		//НачалоКолонки = ?(НачалоКолонки - 1 = 0, СтрДлина(ТекстовыйДокументИсходногоЗапроса.ПолучитьСтроку(НачалоСтроки)) + 1, НачалоКолонки);
		//Элементы.ДеревоЗапросовТекст.УстановитьГраницыВыделения(1, 1, НачалоСтроки, НачалоКолонки - 1);
		//ТекстДо = Элементы.ДеревоЗапросовТекст.ВыделенныйТекст;
		Элементы.ДеревоЗапросовТекст.УстановитьГраницыВыделения(1, 1, НачалоСтроки, НачалоКолонки);
		ТекстДо = Элементы.ДеревоЗапросовТекст.ВыделенныйТекст;
	КонецЕсли; 
	
	КоличествоСимволовДо = СтрДлина(ТекстДо);
	КоличествоСимволовВЗаменяемого = СтрДлина(ИсходныйТекстПакета);
	
	//КоэфЗаменяемого = ?(Прав(ИсходныйТекстПакета, 1) = Символы.ПС, 1 , 0);
	//ТекстПосле = Сред(ИсходныйТекстЗапроса, КоличествоСимволовДо + КоличествоСимволовВЗаменяемого + КоэфДо + КоэфЗаменяемого);
	ТекстПосле = Сред(ИсходныйТекстЗапроса, КоличествоСимволовДо + КоличествоСимволовВЗаменяемого + 1);
	
	НовыйТекстЗапроса = ТекстДо + НовыйТекстПакета + ТекстПосле;
	
	ТекущиеДанные.Текст = НовыйТекстЗапроса;
	
	ТекстовыйДокумент = Новый ТекстовыйДокумент;
	ТекстовыйДокумент.УстановитьТекст(НовыйТекстПакета);
	КоличествоСтрокНовогоПакета = ТекстовыйДокумент.КоличествоСтрок();
	КонецСтроки = НачалоСтроки + КоличествоСтрокНовогоПакета - 1;
